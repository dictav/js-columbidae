{
  "name": "columbidae",
  "moduleName": "ColumbidaeJS",
  "version": "0.1.3",
  "description": "Error reporter for Stackdriver or Sentry",
  "main": "dist/columbidae.js",
  "module": "src/main.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:e2e:phantom",
    "test:cloud": "npm run test:unit && npm run test:e2e:cloud",
    "test:local": "npm run test:unit && npm run test:e2e:phantom",
    "test:unit": "mocha test/unit",
    "test:e2e:phantom": "wdio test/wdio.conf.phantom.js",
    "test:e2e:local": "wdio test/wdio.conf.local.js",
    "test:e2e:cloud": "wdio test/wdio.conf.browserstack.js",
    "build": "npm run build:dist && npm run build:example",
    "build:dist": "rollup --config rollup.config.js",
    "build:example": "rollup --config rollup-example.config.js",
    "watch": "chokidar 'src/*.js' -c 'npm run build'"
  },
  "author": "dictav <dictav@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.17.2",
    "chokidar-cli": "^1.2.0",
    "express": "^4.15.3",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "ngrok": "^2.2.11",
    "reify": "^0.11.24",
    "request": "^2.81.0",
    "rollup": "^0.43.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-uglify": "^2.0.1",
    "wdio-browserstack-service": "^0.1.4",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.8.0"
  }
}
